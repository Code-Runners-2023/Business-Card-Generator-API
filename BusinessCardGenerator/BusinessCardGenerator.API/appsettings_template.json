{
  "ConnectionStrings": {
    "PostgresConnection": "your_postgres_connection_string",
    "AzureConnection": "your_azure_connection_string"
  },
  "JwtSettings": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "SecretKey": "your_jwt_secret_key",
    "ExpirationMinutes": 0
  },
  "AzureSettings": {
    "BlobUrl": "your_blob_url",
    "ResourceGroup": "your_resource_group",
    "Account": "your_account",
    "Container": "your_container"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FrontendUrl": "your_frontend_url"
}
